{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/jentz-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false
  },
  "monorepo": true,
  "root": "apps/jentz-api",
  "projects": {
    "infra": {
      "type": "application",
      "root": "apps/infra",
      "entryFile": "infra",
      "sourceRoot": "apps/infra/bin",
      "compilerOptions": {
        "tsConfigPath": "apps/infra/tsconfig.app.json"
      }
    },
    "jentz-api": {
      "type": "application",
      "root": "apps/jentz-api",
      "entryFile": "main",
      "sourceRoot": "apps/jentz-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/jentz-api/tsconfig.app.json"
      }
    }
  }
}